{
  "private": true,
  "name": "cover",
  "version": "2.3.3",
  "description": "A high quality Component Library built on Vue.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --mode development --config build/webpack.site.config.js --hot --progress --open",
    "build:site": "rimraf docs && cross-env NODE_ENV=production webpack --mode production --config build/webpack.site.config.js --progress",
    "build:vendors": "rimraf vendors && cross-env NODE_ENV=production webpack --mode production --config build/webpack.vendors.config.js --progress",
    "build:components": "rimraf lib && cross-env NODE_ENV=production webpack --mode production --config build/webpack.components.config.js --progress",
    "build": "npm run build:components && npm run build:vendors && npm run build:site",
    "lint": "eslint '{src,site}/**/*.{js,vue}'",
    "lint:fix": "eslint --fix '{src,site}/**/*.{js,vue}'"
  },
  "main": "lib/cover.js",
  "author": "moyu <moyuboy@gmail.com>",
  "files": [
    "lib",
    "vendors",
    "assets",
    "CHANGELOG.md"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
